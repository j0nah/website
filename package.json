{
  "private": true,
  "name": "minimal-blog",
  "description": "Typography driven, feature-rich blogging theme with minimal aesthetics. Includes tags/categories support and extensive features for code blocks such as live preview, line numbers, and line highlighting.",
  "version": "1.3.15",
  "author": "LekoArts",
  "license": "0BSD",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -p 4000",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public -b main"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^6.0.5",
    "disqus-react": "^1.1.5",
    "gatsby": "^5.10.0",
    "gatsby-plugin-feed": "^5.10.0",
    "gatsby-plugin-google-analytics": "^5.11.0",
    "gatsby-plugin-manifest": "^5.10.0",
    "gatsby-plugin-sitemap": "^6.10.0",
    "gatsby-plugin-webpack-statoscope": "^1.0.3",
    "gatsby-remark-autolink-headers": "^6.11.0",
    "gatsby-remark-table-of-contents": "^2.0.0",
    "gatsby-transformer-remark": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scrollspy": "^3.4.3"
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "cross-env": "^7.0.3",
    "gh-pages": "^5.0.0",
    "typescript": "^4.9.5"
  }
}
